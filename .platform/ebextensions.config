Resources:
  AWSEBAutoScalingGroup:
    Type: AWS::AutoScaling::AutoScalingGroup
    Properties:
      VPCZoneIdentifier:
        - subnet-12345678  # Cambia esto por tus subredes reales
      LaunchTemplate:
        LaunchTemplateName: !Ref AWSEBLaunchTemplate
        Version: !GetAtt AWSEBLaunchTemplate.LatestVersionNumber

  AWSEBLaunchTemplate:
    Type: AWS::EC2::LaunchTemplate
    Properties:
      LaunchTemplateData:
        InstanceType: t3.micro  # Cambia esto si necesitas otro tipo de instancia
        SecurityGroupIds:
          - sg-12345678  # Cambia esto por tu grupo de seguridad real
        ImageId: ami-12345678  # Elastic Beanstalk puede seleccionar esto autom√°ticamente
